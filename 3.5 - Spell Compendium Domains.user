  <thing id="xHungBite" name="Hunger Bite" description="Holder" compset="Special">
    <tag group="SpecSource" tag="ClericDom" name="Cleric Domain" abbrev="Cleric Domain"/>
    <bootstrap thing="wBite">
      <autotag group="SpecSource" tag="ClericDom"/>
      </bootstrap>
    <eval phase="PostLevel"><![CDATA[    ~ Rename our natural attacks if they are present and from domain (this is to avoid affecting any natural attacks gained from classes or feats) & also set up our natural attack damage based on size.

    foreach pick in hero from BaseNatWep where "SpecSource.ClericDom"
      if (herofield[tSize].value >= 4) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.4d6_106]
        endif

      elseif (herofield[tSize].value = 3) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.3d6_8]
        endif

      elseif (herofield[tSize].value = 2) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.2d6_104]
        endif

      elseif (herofield[tSize].value = 1) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.1d8_6]
        endif

      elseif (herofield[tSize].value = 0) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.1d6_5]
        endif

      elseif (herofield[tSize].value = -1) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.1d4_4]
        endif

      elseif (herofield[tSize].value = -2) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.1d3_3]
        endif

      elseif (herofield[tSize].value = -3) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.1d2_2]
        endif

      elseif (herofield[tSize].value <= -4) then
        if (each.tagis[thingid.wBite] <> 0) then
           perform each.assign[wMain.1_1]
        endif

      endif
    nexteach]]></eval>
    </thing>
  <thing id="sElfTruStr" name="True Strike" description="You gain temporary, intuitive insight into the immediate future during your next attack. Your next single attack roll (if it is made before the end of the next round) gains a +20 insight bonus. Additionally, you are not affected by the miss chance that applies to attackers trying to strike a concealed target.\n\n{i}Focus{/i}: A small wooden replica of an archery target." compset="Spell" summary="+20 on your next attack roll.">
