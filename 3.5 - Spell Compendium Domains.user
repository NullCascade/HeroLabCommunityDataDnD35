    <usesource source="Frostburn"/>
    <tag group="SecClsAllw" tag="Cold"/>
    <tag group="Domain" tag="Cold" name="Cold" abbrev="Cold"/>
    </thing>
  <thing id="cdCommune" name="Community" description="{b}Granted Power{/b}: Use {i}calm emotions{/i} as a spell-like ability once per day. Gain a +2 bonus on Diplomacy checks.\n\n{b}Community Domain Spells{/b}\n{b}1{/b} &#150; {i}Bless{/i}: Allies gain +1 on attack rolls and saves against fear.\n{b}2{/b} &#150; {i}Status{/i}: Monitors condition, position of allies.\n{b}3{/b} &#150; {i}Prayer{/i}: Allies +1 on most rolls, enemies &#150;1 penalty.\n{b}4{/b} &#150; {i}Tongues{/i}: Speak any language.\n{b}5{/b} &#150; {i}Rary&#146;s Telepathic Bond{/i}: Link lets allies communicate.\n{b}6{/b} &#150; {i}Heroes&#146; Feast{/i}: Food for one creature/level cures and grants combat bonuses.\n{b}7{/b} &#150; {i}RefugeM{/i}: Alters item to transport its possessor to you.\n{b}8{/b} &#150; {i}Mordenkainen&#146;s Magnificent MansionF{/i}: Door leads to extradimensional mansion.\n{b}9{/b} &#150; {i}Heal, Mass{/i}: As {i}heal{/i}, but with several subjects.\n\n{b}Source{/b}: Spell Compendium" compset="ClerDomain" replaces="cdCommun" uniqueness="unique">
    <usesource source="SpellComp"/>
    <tag group="SecClsAllw" tag="Community"/>
    <tag group="Domain" tag="Community" name="Community" abbrev="Community"/>
    </thing>
  <thing id="cdCompete" name="Competition" description="{b}Granted Power (Ex){/b}: You gain a +1 bonus on opposed checks you make.\n\n{b}Competition Domain Spells{/b}\n{b}1{/b} &#150; {i}Remove Fear{/i}: Suppresses fear or gives +4 on saves against fear for one subject + one per 4 levels.\n{b}2{/b} &#150; {i}Zeal&#134;{/i}: You move through foes to attack the enemy you want.\n{b}3{/b} &#150; {i}Prayer{/i}: Allies +1 bonus on most rolls, enemies &#150;1 penalty.\n{b}4{/b} &#150; {i}Divine Power{/i}: You gain attack bonus, +6 to Str, and 1 hp/level.\n{b}5{/b} &#150; {i}Righteous Might{/i}: Your size increases, and you gain combat bonuses.\n{b}6{/b} &#150; {i}Zealot Pact&#134;X{/i}: You automatically gain combat bonuses when you attack someone of opposite alignment.\n{b}7{/b} &#150; {i}Regenerate{/i}: Subject&#146;s severed limbs grow back, cures 4d8 damage +1/level (max +35).\n{b}8{/b} &#150; {i}Moment of Prescience{/i}: You gain insight bonus on single attack roll, check, or save.\n{b}9{/b} &#150; {i}Visage of the Deity, Greater&#134;{/i}: As {i}lesser visage of the deity{/i}, but you become half-celestial or half-fiendish.\n\n{b}Source{/b}: Spell Compendium" compset="ClerDomain" uniqueness="unique">
    <comment><![CDATA[Add spells & Specials for Dom Power]]></comment>
    <usesource source="SpellComp"/>
    <tag group="SecClsAllw" tag="Compete" name="Competition"/>
    <tag group="Domain" tag="Compete" name="Compete" abbrev="Compete"/>
    </thing>
  <thing id="cdCourage" name="Courage" description="{b}Granted Power (Su){/b}: You radiate an aura of courage that grants all allies within 10 feet (including yourself) a +4 morale bonus on saving throws against fear effects. This ability functions only while you are conscious.\n\n{b}Courage Domain Spells{/b}\n{b}1{/b} &#150; {i}Remove Fear{/i}: Suppresses fear or gives +4 on saves against fear for one subject + one per 4 levels.\n{b}2{/b} &#150; {i}Aid{/i}: +1 on attack rolls and saves against fear, 1d8 temporary hp +1/level (max +10).\n{b}3{/b} &#150; {i}Cloak of Bravery{/i}: You and your allies gain a bonus on saves against fear.\n{b}4{/b} &#150; {i}Heroism{/i}: Gives +2 bonus on attack rolls, saves, skill checks.\n{b}5{/b} &#150; {i}Valiant Fury&#134;{/i}: +4 Str, Con; +2 Will saves, extra attack.\n{b}6{/b} &#150; {i}Heroes&#146; Feast{/i}: Food for one creature/level cures and grants combat bonuses.\n{b}7{/b} &#150; {i}Heroism, Greater{/i}: Gives +4 bonus on attack rolls, saves, skill checks; immunity to fear; temporary hp.\n{b}8{/b} &#150; {i}Lion&#146;s Roar&#134;{/i}: Deals 1d8 points of damage/2 levels to enemies; allies get +1 on attacks and saves against fear, temporary hp.\n{b}9{/b} &#150; {i}Cloak of Bravery, Greater&#134;{/i}: You and your allies become immune to fear and get +2 bonus on attacks.\n\n{b}Source{/b}: Spell Compendium, Complete Warrior" compset="ClerDomain" uniqueness="unique">
    <comment><![CDATA[Add spells & Specials for Dom Power]]></comment>
    <usesource source="SpellComp"/>
    <usesource source="CompWarrio"/>
    <tag group="Domain" tag="Courage" name="Courage" abbrev="Courage"/>
    <tag group="SecClsAllw" tag="Courage"/>
    </thing>
  <thing id="cdCraft" name="Craft" description="{b}Granted Power{/b}: You cast conjuration (creation) spells at +1 caster level and gain Skill Focus as a bonus feat for one Craft skill of your choice.\n\n{b}Craft Domain Spells{/b}\n{b}1{/b} &#150; {i}Animate Rope{/i}: Makes a rope move at your command.\n{b}2{/b} &#150; {i}Wood Shape{/i}: Rearranges wooden objects to suit you.\n{b}3{/b} &#150; {i}Stone Shape{/i}: Sculpts stone into any shape.\n{b}4{/b} &#150; {i}Minor Creation{/i}: Creates one cloth or wood object.\n{b}5{/b} &#150; {i}Wall of Stone{/i}: Creates a stone wall that can be shaped.\n{b}6{/b} &#150; {i}Fantastic Machine&#134;{/i}: Creates a machine to perform a single simple task.\n{b}7{/b} &#150; {i}Major Creation{/i}: As {i}minor creation{/i}, plus stone and metal.\n{b}8{/b} &#150; {i}ForcecageM{/i}: Cube or cage of force imprisons all inside.\n{b}9{/b} &#150; {i}Fantastic Machine, Greater&#134;{/i}: Creates a machine to perform multiple tasks.\n\n{b}Source{/b}: Spell Compendium" compset="ClerDomain" uniqueness="unique">
    <comment><![CDATA[Add spells & Specials for Dom Power]]></comment>
    <usesource source="SpellComp"/>
    <tag group="SecClsAllw" tag="Craft"/>
    <tag group="Domain" tag="Craft" name="Craft" abbrev="Craft"/>
    </thing>
  <thing id="cdCreate2" name="Creation" description="{b}Granted Power{/b}: You cast conjuration (creation) spells at +1 caster level.\n\n{b}Creation Domain Spells{/b}\n{b}1{/b} &#150; {i}Create Water{/i}: Creates 2 gallons/level of pure water.\n{b}2{/b} &#150; {i}Minor Image{/i}: As {i}silent image{/i}, plus some sound.\n{b}3{/b} &#150; {i}Create Food and Water{/i}: Feeds three humans (or one horse)/level.\n{b}4{/b} &#150; {i}Minor Creation{/i}: Creates one cloth or wood object.\n{b}5{/b} &#150; {i}Major Creation{/i}: As {i}minor creation{/i}, plus stone and metal.\n{b}6{/b} &#150; {i}Heroes&#146; Feast{/i}: Food for one creature/level cures and grants combat bonuses.\n{b}7{/b} &#150; {i}Permanent ImageM{/i}: Includes sight, sound, and smell.\n{b}8{/b} &#150; {i}True Creation&#134;X{/i}: As {i}major creation{/i}, but permanent.\n{b}9{/b} &#150; {i}Pavilion of Grandeur&#134;{/i}: A feast and a great pavilion are created.\n\n{b}Source{/b}: Spell Compendium" compset="ClerDomain" replaces="cdCreation" uniqueness="unique">
