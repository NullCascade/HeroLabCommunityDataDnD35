elseif (tagis[User.4d6_106] <> 0) then
  field[livename].text = field[name].text & ", 4d6+" & dam & ""
elseif (tagis[User.4d8_307] <> 0) then
  field[livename].text = field[name].text & ", 4d8+" & dam & ""
endif]]></eval>
    </thing>
