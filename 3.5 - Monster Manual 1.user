    <eval phase="Render"><![CDATA[~ Set our Str bonus to damage at superior (200%), high (150%), medium (100%), or low (50%), we default to 150% though.
  var dam as number

  if (tagis[User.DamLowStr] <> 0) then
    dam = round(hero.child[aSTR].field[aModBonus].value * .5, 0, -1)
  elseif (tagis[User.DamMedStr] <> 0) then
    dam = hero.child[aSTR].field[aModBonus].value
  elseif (tagis[User.DamSupStr] <> 0) then
    dam = hero.child[aSTR].field[aModBonus].value * 2
  else
    dam = round(hero.child[aSTR].field[aModBonus].value * 1.5, 0, -1)
  endif

~ Set the damage based on User.? tags
if (tagis[User.1_1] <> 0) then
  field[livename].text = field[name].text & " - 1"
elseif (tagis[User.1d2_2] <> 0) then
  field[livename].text = field[name].text & " - 1d2+" & dam & ""
elseif (tagis[User.1d3_3] <> 0) then
  field[livename].text = field[name].text & " - 1d3+" & dam & ""
elseif (tagis[User.1d4_4] <> 0) then
  field[livename].text = field[name].text & " - 1d4+" & dam & ""
elseif (tagis[User.1d6_5] <> 0) then
  field[livename].text = field[name].text & " - 1d6+" & dam & ""
elseif (tagis[User.1d8_6] <> 0) then
  field[livename].text = field[name].text & " - 1d8+" & dam & ""
elseif (tagis[User.1d10_304] <> 0) then
  field[livename].text = field[name].text & " - 1d10+" & dam & ""
elseif (tagis[User.1d12_404] <> 0) then
  field[livename].text = field[name].text & " - 1d12+" & dam & ""
elseif (tagis[User.2d4_6] <> 0) then
  field[livename].text = field[name].text & " - 2d4+" & dam & ""
elseif (tagis[User.2d6_104] <> 0) then
  field[livename].text = field[name].text & " - 2d6+" & dam & ""
elseif (tagis[User.2d8_204] + tagis[User.2d8_305] <> 0) then
  field[livename].text = field[name].text & " - 2d8+" & dam & ""
elseif (tagis[User.2d10_205] <> 0) then
  field[livename].text = field[name].text & " - 2d10+" & dam & ""
elseif (tagis[User.3d6_8] <> 0) then
