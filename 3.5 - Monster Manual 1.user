      var HD as number
      HD = herofield[tHitDice].value - #totallevelcount[] + #levelcount[Race]
      var DC as number
      DC = 10 + HD/2 + hero.child[aCHA].field[aModBonus].value 
      DC = round(DC, 0, -1)

      field[Value].value += DC</eval>
    </thing>
