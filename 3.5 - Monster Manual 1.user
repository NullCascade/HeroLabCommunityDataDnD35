    <eval phase="First" priority="5000"><![CDATA[~ If we are disabled, do nothing and hide ourselves.
      if (tagis[Helper.SpcDisable] <> 0) then
        perform delete[Helper.ShowSpec]
        done
      endif

      ~ We don't have a STR score because we're incorporeal, and that would just be silly
      perform hero.child[aSTR].assign[Helper.NoScore]]]></eval>
    <eval phase="PostAttr" priority="20000" index="2"><![CDATA[~ If we are disabled, do nothing and hide ourselves.
      if (tagis[Helper.SpcDisable] <> 0) then
        done
      endif

~ We gain our charisma modifier as a deflection bonus to AC, Minimum 1
    hero.child[ArmorClass].field[tACDeflect].value = maximum(hero.child[ArmorClass].field[tACDeflect].value, hero.child[aCHA].field[aModBonus].value)
    hero.child[ArmorClass].field[tACDeflect].value = maximum(hero.child[ArmorClass].field[tACDeflect].value, 1)

~ We add our dex to hit with melee, since we are incorporeal
   hero.child[Attack].field[tAtkMelee].value += hero.child[aDEX].field[aModBonus].value]]></eval>
    </thing>
