      DC = 10 + herofield[tHitDice].value/2 + hero.child[aCON].field[aModBonus].value 
      DC = round(DC, 0, -1)

    if (herofield[tSize].value &gt;= 4) then
       field[livename].text = &quot;Tail Sweep (2d8+&quot; &amp; dambonus &amp; &quot;)&quot;
       field[xSumm].text = &quot;Stand, dam Med or smaller foes in 40 ft rad half circle (DC&quot; &amp; DC &amp; &quot; Ref &#189;).&quot;
    elseif (herofield[tSize].value = 3) then
       field[livename].text = &quot;Tail Sweep (2d6+&quot; &amp; dambonus &amp; &quot;)&quot;
       field[xSumm].text = &quot;Stand, dam Small or smaller foes in 30 ft rad half circle (DC&quot; &amp; DC &amp; &quot; Ref &#189;).&quot;
    else
       perform delete[Helper.ShowSpec]
    endif</eval>
