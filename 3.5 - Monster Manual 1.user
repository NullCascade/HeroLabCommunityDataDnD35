      hero.child[kDisguise].field[kInnate].arrayvalue[index] += 21
      hero.child[kHide].field[kInnate].arrayvalue[index] += 21
      hero.child[kIntim].field[kInnate].arrayvalue[index] += 21
      hero.child[kJump].field[kInnate].arrayvalue[index] += 21
      hero.child[kKnowArcan].field[kInnate].arrayvalue[index] += 21
      hero.child[kKnowPlane].field[kInnate].arrayvalue[index] += 21
      hero.child[kKnowRel].field[kInnate].arrayvalue[index] += 21
      hero.child[kListen].field[kInnate].arrayvalue[index] += 21
      hero.child[kMoveSil].field[kInnate].arrayvalue[index] += 21
      hero.child[kSearch].field[kInnate].arrayvalue[index] += 21
