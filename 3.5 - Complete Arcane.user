      </bootstrap>
    <bootstrap thing="xPseuTrStr">
      <autotag group="SpecSource" tag="Pseudonatu"/>
      </bootstrap>
    <bootstrap thing="xPseuAltFo">
      <autotag group="SpecSource" tag="Pseudonatu"/>
      </bootstrap>
    <bootstrap thing="xDamRsAcid">
      <autotag group="SpecSource" tag="Pseudonatu"/>
      </bootstrap>
    <bootstrap thing="typOutside">
      <autotag group="User" tag="OverNoUseT"/>
      </bootstrap>
    <bootstrap thing="stypAugmen"></bootstrap>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      ~ Set up the amount of damage we resist
      var damage as number
      if (herofield[tHitDice].value >= 12) then
        damage = 15
      elseif (herofield[tHitDice].value >= 8) then
        damage = 10
      else
        damage = 5
        endif

      #applyresist[xDamRsAcid, damage]
      #applyresist[xDamRsElec, damage]

      ~ Set up our damage reduction
      if (herofield[tHitDice].value >= 12) then
        damage = 10
      elseif (herofield[tHitDice].value >= 4) then
        damage = 5
      else
        damage = 0
        endif
      #applydr[xDamRdMag, damage]

      ~ Set up our spell resistance
      damage = herofield[tHitDice].value + 10
      damage = minimum(damage, 25)
      #applysr[damage]

      ~ If our intelligence minus starting modifiers is < 3, make it 3
      var intel as number
      perform hero.child[aINT].setfocus
      intel = focus.field[aValue].value + focus.field[aStartMod].value
      if (intel < 3) then
        focus.field[aNormMod].value += 3 - intel
        endif]]></eval>
    <eval phase="First" priority="50" index="2"><![CDATA[~ Go through all type specials and assign a tag to disable them. Also set the hTotal field to <> 0 so that bootstrap conditions will fail. 
   foreach pick in hero from BaseSpec where "thingid.typ? & !thingid.typOutside"
     perform each.assign[Helper.SpcDisable]
     each.field[hTotal].value = 1

~ Even though we are no longer that type, we do not recalculate BAB, saves, or anything else.
     perform each.assign[User.OverUseTyp]
   nexteach

~ Finally, delete all Race.typWhatever tags and add back one for the Outsider type.
  perform hero.delete[Race.typ?]
  perform hero.assign[Race.typOutside]]]></eval>
    <exprreq message="Incorporeal subtype forbidden.">tagis[Race.stypIncorp] = 0</exprreq>
    </thing>
