    <tag group="AbilType" tag="SpellLike"/>
    <tag group="SpecType" tag="SpellLike"/>
    <exprreq message="Ability to use Lesser Invocations required."><![CDATA[hero.childfound[xInvoke].field[Value].value >= 6]]></exprreq>
    </thing>
  <thing id="cWrlDevSig" name="Devil&apos;s Sight (Least; 2nd)" description="You gain the visual acuity of a devil for 24 hours. You can see normally in darkness and magical darkness out to 30 feet." compset="CustomSpec" summary="See normally in darkness and magical darkness." uniqueness="unique">
    <tag group="SpecSource" tag="cHelpWrl"/>
    <tag group="AbilType" tag="SpellLike"/>
    <tag group="SpecType" tag="Sense"/>
