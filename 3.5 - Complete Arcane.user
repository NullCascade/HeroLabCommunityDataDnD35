         perform assign[User.ElemCold]
      elseif (field[fChosen].chosen.tagis[User.ElemElec] <> 0) then
         perform assign[User.ElemElec]
      elseif (field[fChosen].chosen.tagis[User.ElemFire] <> 0) then
         perform assign[User.ElemFire]
      endif]]></eval>
    <exprreq message="Knowledge (arcana) 5 ranks required."><![CDATA[#skillranks[kKnowArcan] >= 5]]></exprreq>
    <exprreq message="Any metamagic feat required."><![CDATA[tagcount[fCategory.Metamagic] - tagcount[HasFeat.fNolethSub] <> 0]]></exprreq>
    </thing>
  <thing id="fDracHerit" name="Draconic Heritage" description="You have greater connection with your distant draconic bloodline.\n\n{b}Prerequisite{/b}: Sorcerer level 1st.\n\n{b}Benefit{/b}: Choose one dragon from the Draconic Heritage list below and gain the indicated skill as a class skill. This is your draconic heritage, which cannot be changed once the feat has been taken. Half-dragons must choose the same dragon kind as their dragon parent.\n\n{b}Dragon      Energy       Skill{/b}\nBlack         Acid            Hide\nBlue           Electricity  Listen\nGreen        Acid            Move Silently\nRed            Fire             Intimidate\nWhite        Cold            Balance\nBrass        Fire             Gather Information\nBronze      Electricity Survival\nCopper      Acid           Hide\nGold           Fire             Heal\nSilver        Cold            Disguise\n\nIn addition, you gain a bonus on saving throws against sleep and paralysis, as well as spells and abilities with the energy type of your Draconic Heritage. This bonus is equal to the number of draconic feats you have.\n\n{b}Source{/b}: Complete Arcane" compset="Feat" uniqueness="useronce">
    <fieldval field="ftCandExpr" value="User.PickDrag"/>
    <usesource source="CompArcane" parent="p35" name="Complete Arcane"/>
    <tag group="fCategory" tag="Draconic"/>
    <bootstrap thing="xDracHerit"></bootstrap>
    <eval phase="UserFirst" priority="10000"><![CDATA[
      ~ If we're disabled, do nothing
      if (tagis[Helper.FtDisable] <> 0) then
        done
        endif

      ~ If we haven't chosen anything, get out now
      if (field[fChosen].ischosen = 0) then
        done
      endif

      ~ Set our 'short name'
      if (field[fChosen].ischosen <> 0) then
        field[fShortName].text = "Draconic Heritage: " & field[fChosen].chosen.field[name].text
      endif

      ~ Assign an appropriate dragon tag and skill tag to ourselves depending on our choice.
      if (field[fChosen].chosen.tagis[User.DragBlack] <> 0) then
         perform assign[User.DragBlack]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kHide]

      elseif (field[fChosen].chosen.tagis[User.DragBlue] <> 0) then
         perform assign[User.DragBlue]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kListen]

      elseif (field[fChosen].chosen.tagis[User.DragBrass] <> 0) then
         perform assign[User.DragBrass]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kGatherInf]

      elseif (field[fChosen].chosen.tagis[User.DragBronze] <> 0) then
         perform assign[User.DragBronze]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kSurvival]

      elseif (field[fChosen].chosen.tagis[User.DragCopper] <> 0) then
         perform assign[User.DragCopper]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kHide]

      elseif (field[fChosen].chosen.tagis[User.DragGold] <> 0) then
         perform assign[User.DragGold]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kHeal]

      elseif (field[fChosen].chosen.tagis[User.DragGreen] <> 0) then
         perform assign[User.DragGreen]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kMoveSil]

      elseif (field[fChosen].chosen.tagis[User.DragRed] <> 0) then
         perform assign[User.DragRed]
         perform hero.childfound[cHelpSor].assign[ClassSkill.kIntim]

      elseif (field[fChosen].chosen.tagis[User.DragSilver] <> 0) then
