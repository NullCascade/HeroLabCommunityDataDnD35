       endif]]></eval>
    <exprreq message="Alienist level 6th required."><![CDATA[tagcount[Classes.Alienist] >= 6]]></exprreq>
    </thing>
  <thing id="cvAlienFam" name="Alienist with Familiar" description="Although alienist does not grant a familiar, it stacks if you already have a familiar. Add this varient if you wish to stack your levels, it removes nothing.\n\n{b}Source{/b}: Complete Arcane" compset="ClassVary" uniqueness="unique">
    <fieldval field="cvArcFStar" value="1"/>
