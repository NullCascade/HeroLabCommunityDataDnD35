    <eval phase="Final" priority="10100" index="2"><![CDATA[var level as number
level = field[xTotalLev].value

var bonus as number

if (level >= 10) then
   bonus = 4
elseif (level >= 8) then
   bonus = 3
elseif (level >= 6) then
   bonus = 2
elseif (level >= 4) then
   bonus = 1
else
   bonus = 0
endif

  var base as number
~ Go though each of our Arcane casting classes, find the highest DC
  foreach pick in hero from BaseClHelp where "CasterSrc.Arcane"
    base = maximum(base, each.field[cSplSaveDC].value)
  nexteach

   var DC as number
   DC = base + 4

field[xSumm].text = "1/day, Perma charm up to Large target in 100 ft, " & bonus & " at a time (DC" & DC & " Will neg)."
field[livename].text = "Eternal Charm (" & bonus & ")"]]></eval>
    </thing>
  <thing id="cMnBESpPow" name="Enchantment Spell Power" description="At 6th level and higher, a mindbender casts more powerful enchantment spells: He adds 2 to his caster level when casting any enchantment spell. This increase improves to 4 at 10th level." compset="ClSpecial" summary="+X to CL for Enchantment spells.">
