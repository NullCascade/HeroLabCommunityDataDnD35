    <tag group="Helper" tag="MatWepOK" name="MatWepOK" abbrev="MatWepOK"/>
    <tag group="Helper" tag="MastCstInc" name="MastCstInc" abbrev="MastCstInc"/>
    <eval phase="First" priority="5000"><![CDATA[
      ~ Set our custom description
      if (container.tagis[mClass.Light] <> 0) then
        field[eMatDesc].text = "Damage Reduction 1/-."
      elseif (container.tagis[mClass.Medium] <> 0) then
        field[eMatDesc].text = "Damage Reduction 2/-."
      elseif (container.tagis[mClass.Heavy] <> 0) then
        field[eMatDesc].text = "Damage Reduction 3/-."
      else
        field[eMatDesc].text = "Ignore hardness less than 20."
        endif]]></eval>
