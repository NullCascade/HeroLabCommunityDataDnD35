    </thing>
  <thing id="fDraResist" name="Draconic Resistance" description="Your bloodline hardens your body against the energy type of your progenitor.\n\n{b}Prerequisite{/b}: Draconic Heritage. \n\n{b}Benefit{/b}: You gain resistance to the energy type of your draconic heritage equal to three times the number of draconic feats you currently have (including draconic feats you take after gaining this feat).\n\n{b}Source{/b}: Complete Arcane" compset="Feat" uniqueness="useronce">
    <usesource source="CompArcane" parent="p35" name="Complete Arcane"/>
    <tag group="fCategory" tag="Draconic"/>
    <bootstrap thing="xDamRsAcid"></bootstrap>
    <bootstrap thing="xDamRsCold"></bootstrap>
    <bootstrap thing="xDamRsElec"></bootstrap>
    <bootstrap thing="xDamRsFire"></bootstrap>
    <eval phase="UserPostLv" priority="10000"><![CDATA[var resist as number
resist = hero.tagcount[fCategory.Draconic] * 3

    if (hero.childfound[fDracHerit].tagis[User.DragBlack] + hero.childfound[fDracHerit].tagis[User.DragGreen] + hero.childfound[fDracHerit].tagis[User.DragCopper] <> 0) then
     field[livename].text = "Draconic Resistance : Acid" 
     #applyresist[xDamRsAcid, resist]

    elseif (hero.childfound[fDracHerit].tagis[User.DragBlue] + hero.childfound[fDracHerit].tagis[User.DragBronze] <> 0) then
     field[livename].text = "Draconic Resistance : Electricity" 
     #applyresist[xDamRsElec, resist]

    elseif (hero.childfound[fDracHerit].tagis[User.DragSilver] + hero.childfound[fDracHerit].tagis[User.DragWhite] <> 0) then
     field[livename].text = "Draconic Resistance : Cold" 
     #applyresist[xDamRsCold, resist]

    elseif (hero.childfound[fDracHerit].tagis[User.DragRed] + hero.childfound[fDracHerit].tagis[User.DragBrass] + hero.childfound[fDracHerit].tagis[User.DragGold] <> 0) then
     field[livename].text = "Draconic Resistance : Fire" 
     #applyresist[xDamRsFire, resist]

    endif]]></eval>
    <exprreq message="Draconic Heritage required."><![CDATA[#hasfeat[fDracHerit] <> 0]]></exprreq>
    </thing>
  <thing id="xDracClaw" name="Draconic Claw" description="This should be hidden" compset="Special">
