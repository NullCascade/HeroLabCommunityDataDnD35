   elseif (level >= 5) then
      hero.child[xFly].field[Value].value = maximum(hero.child[xFly].field[Value].value, 40)
      perform hero.child[xFly].assign[Maneuver.Average]
      hero.child[xFly].field[livename].text = "Raptoran Fly (Fatigue)"
      hero.child[xFly].field[xSumm].text = "Raptorans of 5+ HD can fly for " & conbonus & " rounds before becoming fatigued."
