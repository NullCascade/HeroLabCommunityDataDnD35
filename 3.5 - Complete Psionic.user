    <bootstrap thing="xPsyAura"></bootstrap>
    </thing>
  <thing id="xPsyAuraA" name="Psychic Aura, Attack" description="You and all allies within your aura gain a +1 morale bonus on attack rolls and damage rolls. This bonus increases by 1 for every five class levels you have." compset="Special" uniqueness="unique">
    <fieldval field="xSource" value="Class"/>
    <fieldval field="hName1" value="Active"/>
    <usesource source="CompPsi" parent="p35" name="Complete Psionics"/>
    <tag group="Helper" tag="ShowCharge"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="SpecType" tag="Attack" name="Attack" abbrev="Attack"/>
    <eval phase="PostLevel"><![CDATA[   var aura as number

    aura = #levelcount[DivineMnd]/2
    aura = round(aura, 0, 1)
    aura = aura*5

      ~ Set our name properly
      field[livename].text = field[name].text & " "& aura &"'"

   var bonus as number

    bonus = #levelcount[DivineMnd]/5
    bonus = round(bonus, 0, -1)
    bonus += 1

      ~ If Aura is active, add bonus
      if (field[hIsOn1].value <> 0) then
hero.child[Attack].field[BonMorale].value = maximum(hero.child[Attack].field[BonMorale].value, bonus)
      endif]]></eval>
    </thing>
  <thing id="xPsyAuraD" name="Psychic Aura, Defense" description="You and all allies within your aura gain a +1 morale bonus to Armor Class. This bonus increases by 1 for every five class levels you have." compset="Special" uniqueness="unique">
    <fieldval field="xSource" value="Class"/>
    <fieldval field="hName1" value="Active"/>
    <usesource source="CompPsi" parent="p35" name="Complete Psionics"/>
    <tag group="SpecType" tag="Defense"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="Helper" tag="ShowCharge"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[   var aura as number

    aura = #levelcount[DivineMnd]/2
    aura = round(aura, 0, 1)
    aura = aura*5

      ~ Set our name properly
      field[livename].text = field[name].text & " "& aura &"'"

   var bonus as number

    bonus = #levelcount[DivineMnd]/5
    bonus = round(bonus, 0, -1)
    bonus += 1

      ~ If Aura is active, add bonus
      if (field[hIsOn1].value <> 0) then
hero.child[ArmorClass].field[BonMorale].value = maximum(hero.child[ArmorClass].field[BonMorale].value, bonus)
      endif]]></eval>
    </thing>
  <thing id="xPsyAuraP" name="Psychic Aura, Perception" description="You and all allies within your aura gain a +2 morale bonus on Initiative, Listen, and Spot checks. This bonus increases by 1 for every five class levels you have." compset="Special" uniqueness="unique">
    <fieldval field="xSource" value="Class"/>
    <fieldval field="hName1" value="Active"/>
    <usesource source="CompPsi" parent="p35" name="Complete Psionics"/>
    <tag group="SpecType" tag="Sense"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="Helper" tag="ShowCharge"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[   var aura as number

    aura = #levelcount[DivineMnd]/2
    aura = round(aura, 0, 1)
    aura = aura*5

      ~ Set our name properly
      field[livename].text = field[name].text & " "& aura &"'"

   var bonus as number
   
    bonus = #levelcount[DivineMnd]/5
    bonus = round(bonus, 0, -1)
    bonus += 2

      ~ If Aura is active, add bonus
      if (field[hIsOn1].value <> 0) then
       #skillbonus[kListen] = #skillbonus[kListen] + bonus
       #skillbonus[kSpot] += bonus
       hero.child[Initiative].field[BonMorale].value = hero.child[Initiative].field[BonMorale].value + bonus
      endif]]></eval>
    </thing>
