  <thing id="cDivGrace" name="Divine Grace" description="Beginning at 4th level, you gain a bonus equal to your Charisma bonus (if any) on all saving throws." compset="ClSpecial" summary="Gain a bonus equal to your Charisma bonus to all saving throws.">
    <usesource source="CompPsi" parent="p35" name="Complete Psionics"/>
    <tag group="SpecSource" tag="cHelpDvM"/>
    <tag group="ClSpecWhen" tag="4"/>
    <eval phase="UserPostAt">var bonus as number
      bonus = hero.child[aCHA].field[aModBonus].value 

hero.child[vRef].field[Bonus].value = hero.child[vRef].field[Bonus].value + bonus

hero.child[vFort].field[Bonus].value = hero.child[vFort].field[Bonus].value + bonus

hero.child[vWill].field[Bonus].value = hero.child[vWill].field[Bonus].value + bonus</eval>
    </thing>
  <thing id="xChngAura" name="Change Aura" description="At 8th level, you gain the ability to change between your mantle auras much more quickly. It takes only a standard action to change between auras. You can switch auras as a move action at 14th level, and at 18th level you can change your aura as a swift action." compset="Special" summary="Change your aura more swiftly." uniqueness="unique">
    <fieldval field="xSource" value="Class"/>
    <usesource source="CompPsi" parent="p35" name="Complete Psionics"/>
    <tag group="Helper" tag="ShowSpec" name="ShowSpec" abbrev="ShowSpec"/>
    <tag group="SpecType" tag="Sense"/>
    <tag group="AbilType" tag="Super"/>
    <eval phase="PostLevel"><![CDATA[

      ~ Set our name properly

      if (#levelcount[DivineMnd] <14) then
       field[livename].text = field[name].text & " (standard action)"
      elseif (#levelcount[DivineMnd] <18) then
       field[livename].text = field[name].text & " (move action)"
      else
       field[livename].text = field[name].text & " (swift action)"
      endif]]></eval>
    </thing>
