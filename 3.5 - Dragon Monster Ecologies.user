  <thing id="xKyDRdSilv" name="Damage Reduction, Silver" description="{b}Damage Reduction (Ex{/b}): A favored spawn of Kyuss gains damage reduction 5/silver. If the favored spawn has 10 or more Hit Dice, this increases to damage reduction 10/silver." compset="Special" uniqueness="unique">
    <comment>Added by Bodrin 20/03/2011</comment>
    <usesource source="DME"/>
    <tag group="AbilType" tag="Extra"/>
    <tag group="SpecSource" tag="FSpKyuss"/>
    <tag group="Helper" tag="ShowSpec" name="ShowSpec" abbrev="ShowSpec"/>
    <tag group="SpecType" tag="DR" name="Damage Reduction" abbrev="Damage Reduction"/>
    <eval phase="Final" priority="10000"><![CDATA[
      call CalcValue

      ~ Use our value field as the amount of damage we can resist
      field[livename].text = "Damage Reduction (" & field[Value].value & "/silver)"]]></eval>
    </thing>
