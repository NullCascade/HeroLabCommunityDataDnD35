    <tag group="AlgnReq" tag="ChaotEvil"/>
    <tag group="SpecSource" tag="FSpKyuss"/>
    <tag group="TemplHelp" tag="Inherited"/>
    <bootstrap thing="xTurnRes">
      <autotag group="Value" tag="3"/>
      </bootstrap>
    <bootstrap thing="typUndead"></bootstrap>
    <bootstrap thing="xKyWormBur"></bootstrap>
    <bootstrap thing="fImpUnarm">
      <autotag group="thing" tag="skipprereq"/>
      </bootstrap>
    <bootstrap thing="xKyCrSpawn"></bootstrap>
    <bootstrap thing="xKyussFear"></bootstrap>
    <bootstrap thing="xKyFstHeal">
      <autotag group="Value" tag="5"/>
      </bootstrap>
    <bootstrap thing="xKyInfSkin"></bootstrap>
    <bootstrap thing="xKyDRdSilv">
      <autotag group="Value" tag="5"/>
      </bootstrap>
    <bootstrap thing="xKyussGift"></bootstrap>
    <bootstrap thing="xKyNxBreat"></bootstrap>
    <eval phase="PostLevel" priority="10000"><![CDATA[
      var result as number

      ~ We don't have a CON score because we're undead, and above that sort of
      ~ petty nonsense
      result = hero.child[aCON].assign[Helper.NoScore]

      ~ Set up our damage reduction 9 HD or lower
      var damage as number
      #applydr[xKyDRdSilv, 5]
     
      ~ +3 Turn Resistance
      hero.child[xTurnRes].field[Value].value = maximum(hero.child[xTurnRes].field[Value].value, 3)]]></eval>
    </thing>
