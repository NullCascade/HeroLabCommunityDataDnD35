    <tag group="AbilType" tag="Super"/>
    <tag group="SpecType" tag="Attack"/>
    <tag group="SpecSource" tag="FSpKyuss" name="Favoured Spawn of Kyuss"/>
    </thing>
  <thing id="xKyussFear" name="Fear Aura" description="{b}Fear Aura (Su){/b{: A spawn of Kyuss continuously radiates a fear effect. This ability functions like a fear spell (caster level 7th, Will save DC 14 + the favored spawn&apos;s Charisma modifier), except that it affects all creatures within a 40-foot radius. \n\nAny creature that makes a successful saving throw against the effect cannot be affected again by the fear aura of that favored spawn of Kyuss for 24 hours." compset="Special" uniqueness="unique">
    <comment>Added by Bodrin 20/03/2011</comment>
    <usesource source="DME"/>
    <tag group="SpecSource" tag="FSpKyuss"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="SpecType" tag="Defense"/>
    <tag group="Helper" tag="ShowSpec" name="ShowSpec" abbrev="ShowSpec"/>
    </thing>
  <thing id="xKyFoulEmb" name="Foul Embrace" description="{b}Foul Embrace (Su){/b}: By pressing its face against a helpless victim, the favored spawn of Kyuss can infest the victim with a rain of 2d6 worms. This ability is treated the same as the create spawn ability, but a victim slain by the resulting infestation rises as a favored spawn of Kyuss rather than a normal zombie." compset="Special" uniqueness="unique">
    <comment>Added by Bodrin 20/03/2011</comment>
    <usesource source="DME"/>
    <tag group="Helper" tag="ShowSpec"/>
