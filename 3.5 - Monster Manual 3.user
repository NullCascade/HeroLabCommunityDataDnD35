      perform hero.child[aCON].assign[Helper.NoScore]]]></eval>
    <eval phase="First" priority="5000" index="3">var index as number
      index = field[cClsIndex].value

      hero.child[kHide].field[kInnate].arrayvalue[index] += 13
      hero.child[kIntim].field[kInnate].arrayvalue[index] += 13
