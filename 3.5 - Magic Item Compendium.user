    <bootstrap thing="xAxAnVirCM">
      <containerreq phase="First" priority="500"><![CDATA[count:User.PickDCoMor >= 1]]></containerreq>
      </bootstrap>
    <bootstrap thing="xAxAnVirBl">
      <containerreq phase="First" priority="500"><![CDATA[count:User.PickDCoMor >= 1]]></containerreq>
      </bootstrap>
    <bootstrap thing="xAxAnVirHa">
      <containerreq phase="First" priority="500"><![CDATA[count:User.PickDCoMor >= 2]]></containerreq>
