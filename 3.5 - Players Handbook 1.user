  <thing id="pcnHaste" name="Hasted" description="Select this adjustment to be under the effects of a Haste spell" compset="InPlay">
    <tag group="Helper" tag="AdjCond"/>
    <tag group="Helper" tag="NoIncr"/>
    <bootstrap thing="xCorrector"></bootstrap>
    <bootstrap thing="xInitiator"></bootstrap>
    <eval phase="PostAttr" priority="11000" index="2"><![CDATA[~ This script fixes PP totals for Psionic characters

var attrbonus as number

    foreach pick in hero from BaseClHelp where "Helper.Manifester"
      attrbonus = eachpick.linkage[psiattr].field[aModBonus].value

      if (attrbonus <= 0) then
        ~ This part undoes the error on the total power points.
        hero.child[Psionics].field[tPPTotal].value -= eachpick.field[cPsiLevMax].value

        ~ It's too late to stop the points from being forwarded from this class, but this part at least fixes the display on our class tab.
        eachpick.field[cPsiPPMax].value -= eachpick.field[cPsiLevMax].value
        endif
      nexteach]]></eval>
    <eval phase="PreLevel" priority="10000"><![CDATA[
      ~ If we're not enabled, get out now
      if (field[pIsOn].value = 0) then
        done
        endif

      ~ Add a +30 or our speed enhancement bunus, whichever is lower
      if (hero.child[Speed].field[tSpeed].value >= 30) then
       hero.child[Speed].field[BonEnhance].value = maximum(hero.child[Speed].field[BonEnhance].value, 30)
      elseif (hero.child[Speed].field[tSpeed].value < 30) then
       var bonus as number
       bonus = hero.child[Speed].field[tSpeed].value
        hero.child[Speed].field[BonEnhance].value = maximum(hero.child[Speed].field[BonEnhance].value, bonus)
     endif

      ~ Add to our attack bonus
      hero.child[Attack].field[Bonus].value += 1
      
      ~ Add to our armor class
      hero.child[ArmorClass].field[tACDodge].value += 1

      ~ Add to our Ref saves
      container.child[vRef].field[Bonus].value = container.child[vRef].field[Bonus].value + 1

      ~ Add an extra attack to our weapons
      foreach pick in hero from BaseWep where "!Helper.ExtraHigh"
        perform eachpick.assign[Helper.ExtraHigh]
      nexteach]]></eval>
    <eval phase="First" priority="12000" index="3"><![CDATA[~ This script adds the tag "portal.ClsBook" to Wizard's 0-level spells

if (#levelcount[Wizard] >= 1) then
 foreach pick in hero from BaseSpell where "Helper.WizBoot"
  perform eachpick.assign[portal.ClsBook]
 nexteach
endif]]></eval>
    <eval phase="Final" priority="18000" index="4"><![CDATA[~ This script adds Corrupt spells from BoVD to classes that can use them.

doneif (hero.tagis[source.BoVD] + hero.tagis[source.HeroHorror] = 0)
~doneif (hero.tagis[Alignment.Good] <> 0)

foreach pick in hero from BaseClHelp where "CasterType.MemAll"
 if (each.field[cSplMaxLvl].value = 9) then
  eachpick.field[cSpellExpr].text &= " | sClass.Corrupt"
 elseif (each.field[cSplMaxLvl].value = 5) then
  eachpick.field[cSpellExpr].text &= " | sClass.Corrupt & (sLevel.1 | sLevel.2 | sLevel.3 | sLevel.4 | sLevel.5)"
 elseif (each.field[cSplMaxLvl].value = 4) then
  eachpick.field[cSpellExpr].text &= " | sClass.Corrupt & (sLevel.1 | sLevel.2 | sLevel.3 | sLevel.4)"
 endif
nexteach


foreach pick in hero from BaseClHelp where "CasterType.MemBook"
 if (each.field[cSplMaxLvl].value = 4) then
  eachpick.field[cSplBkExpr].text &= " | sClass.Corrupt & (sLevel.1 | sLevel.2 | sLevel.3 | sLevel.4)"
  eachpick.field[cSpellExpr].text &= "| sClass.Corrupt"
 elseif (each.field[cSplMaxLvl].value = 3) then
  eachpick.field[cSplBkExpr].text &= " | sClass.Corrupt & (sLevel.1 | sLevel.2 | sLevel.3)"
  eachpick.field[cSpellExpr].text &= "| sClass.Corrupt"
 else
  eachpick.field[cSplBkExpr].text &= " | sClass.Corrupt"
  eachpick.field[cSpellExpr].text &= "| sClass.Corrupt"
 endif
nexteach]]></eval>
    </thing>
  <thing id="xCorrector" name="Corrector" description="This is where any Eval Scripts that you want to be run on every character to be run. Since this is bootstrapped to a condition and that is always on every hero, we can do global effects from here.\n\nCurrently this is home to a script to erase Template tags from the hero after Post Attributes 5000" compset="Special">
    <eval phase="PostAttr" priority="5000"><![CDATA[
   ~ Template tags are messing up our DC calculations. I don't think they are used for anything, so I am just going to delete them here.
   perform hero.delete[Classes.Template]]]></eval>
    </thing>
