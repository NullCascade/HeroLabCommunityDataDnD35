  <thing id="xTKPoison" name="Poison" description="Injury, Fortitude DC 11, initial damage 1d6 Dex, secondary damage paralysis for 2d6 minutes. A thri-kreen can use its poison bite once per day. The save DC is Constitution-based." compset="Special">
    <fieldval field="hTotal" value="1"/>
    <usesource source="SRDPsionic"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <tag group="Helper" tag="ShowCharge"/>
    <tag group="Helper" tag="ShowSpec" name="ShowSpec" abbrev="ShowSpec"/>
    <eval phase="UserPostAt"><![CDATA[      var DC as number
      var uses as number

      DC = 10 + hero.child[aCON].field[aModBonus].value
      uses = hero.childfound[xTKPoison].field[hTotal].value

      field[xSumm].text = "Bite - DC" & DC & " - paralysis 2d6 minutes " & uses & "/day."]]></eval>
    </thing>
  <thing id="xTKPSA" name="Thri-Kreen Psi-like Abilities" compset="Special" summary="Gain psi-like abilities.">
    <usesource source="SRDPsionic"/>
    <tag group="Helper" tag="ShowSpec"/>
    <bootstrap thing="xChamPLA"></bootstrap>
    <bootstrap thing="xKnowPLA"></bootstrap>
    <bootstrap thing="xGCAPLA"></bootstrap>
    <bootstrap thing="xClawPLA"></bootstrap>
    <eval phase="First" priority="10000"><![CDATA[var conceal as number
var claw as number

conceal = hero.childfound[xGCAPLA].field[hTotal].value
claw = hero.childfound[xClawPLA].field[hTotal].value

if (conceal = claw) then

field[CustDesc].text = "3/day: {i}chameleon{/i}, {i}know direction and location{/i}; "& conceal &"/day {i}greater concealing amorpha{/i}, {i}metaphysical claw{/i}"

else

field[CustDesc].text = "3/day: {i}chameleon{/i}, {i}know direction and location{/i}; "& conceal &"/day {i}greater concealing amorpha{/i}; "& claw &"/day {i}metaphysical claw{/i}"

endif]]></eval>
    </thing>
  <thing id="xLeap" name="Leap" description="A thri-kreen is a natural jumper. It has a +30 racial bonus on Jump checks." compset="Special" summary="+30 racial bonus on Jump checks.">
