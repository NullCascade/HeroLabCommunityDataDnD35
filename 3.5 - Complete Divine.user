    <fieldval field="usrCandid1" value="User.PickFaS"/>
    <tag group="Helper" tag="SpecUp"/>
    <tag group="ClSpecWhen" tag="20"/>
    <tag group="AbilType" tag="Super"/>
    <tag group="ChooseSrc1" tag="Container"/>
    <tag group="SpecSource" tag="cHelpFaS"/>
