    <exprreq message="Any neutral alignment required."><![CDATA[hero.tagis[Alignment.NeutralGE] + hero.tagis[Alignment.NeutralLC] <> 0]]></exprreq>
    <prereq message="Ability to cast any {i}summon nature&apos;s ally{/i} spell required.">
      <validate><![CDATA[ @valid = 0
~ Druids can cast SNA spells from 1st level.
    validif (tagcount[Classes.Druid] <> 0)

~ Rangers must be 4th level with an attribute bonus, or else 6th level to cast thier first Healing spell (cure light wounds).
 if (hero.childfound[cHelpRgr].field[cMagicLev].value >= 6) then
    @valid = 1
 elseif (hero.childfound[cHelpRgr].field[cMagicLev].value >= 4) then
   if (hero.childfound[cHelpRgr].field[cSplAttBon].value >= 1) then
    @valid = 1
   endif
 endif]]></validate>
      </prereq>
    </thing>
  <thing id="fSwimFish" name="Swim Like a Fish" description="You can breathe and swim underwater with grace.\n\n{b}Prerequisite{/b}: Ability to wild shape.\n\n{b}Benefit{/b}: You can spend one wild shape to grow gills, enabling you to breathe underwater (while retaining your ability to breathe air). Webbing also grows between your fingers and between your toes, granting you a Swim speed of 40 ft. and a +8 bonus on your Swim checks. The effect lasts for one hour.\n\n{b}Source{/b}: Complete Divine" compset="Feat" summary="Burn wild shape, gain 40 ft Swim speed and ability to breathe water for 1 hr." uniqueness="useronce">
    <usesource source="CompDivine"/>
    <tag group="User" tag="SwimFish" name="Swim Like a Fish"/>
    <tag group="fCategory" tag="Wild"/>
    <bootstrap thing="xSwimFish"></bootstrap>
    <eval phase="Final" priority="11000"><![CDATA[  if (field[hIsOn1].value <> 0) then
      hero.child[xSwim].field[Value].value = maximum(40, hero.child[xSwim].field[Value].value)
  endif]]></eval>
    </thing>
  <thing id="fTranDiSpl" name="Transdimensional Spell" description="You can cast spells that affect targets lurking in coexistent planes and extradimensional spaces whose entrances fall within the spell&#146;s area.\n\n{b}Benefit{/b}: A transdimensional spell has full, normal effect upon incorporeal creatures, creatures on the Ethereal Plane or Plane of Shadow, and creatures within an extradimensional space in the spell&#146;s area. Such creatures include ethereal creatures, creatures that are {i}blinking{/i} or shadow walking, manifested ghosts, and creatures within the extradimensional space of a {i}rope trick{/i} or {i}portable hole{/i}.\n\nYou must be able to perceive a creature to target it with a spell, but you do not need to perceive a creature to catch it in the area of a burst, cone, emanation, or spread.\n\nA transdimensional spell uses up a spell slot one level higher than the spell&#146;s actual level.\n\n{b}Normal{/b}: Only force spells and effects can affect ethereal creatures, and no Material Plane attack affects creatures on the Plane of Shadow or in an enclosed extradimensional space. There is a 50% chance that any spell other than a force effect fails against an incorporeal creature.\n\n{b}Source{/b}: Complete Divine" compset="Feat" summary="Spells affect incorporeal foes and those on other planes. +1 Level." uniqueness="useronce">
    <usesource source="CompDivine"/>
    <usesource source="CompArcane"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="fCategory" tag="Metamagic"/>
    </thing>
  <thing id="fTrueBelie" name="True Believer" description="Your deity rewards your unquestioning faith and dedication.\n\n{b}Prerequisite{/b}: Must choose a single deity to worship. Must be within one step of that god&#146;s alignment.\n\n{b}Benefit{/b}: Once per day when you are about to make a saving throw you may declare that you are using this feat to gain a +2 insight bonus on that saving throw.\n\nThis feat also allows you to use a relic (see relics, page 88) of the deity you worship.\n\n{b}Source{/b}: Complete Divine" compset="Feat" summary="1/day, +2 insight bonus on one saving throw." uniqueness="useronce">
    <fieldval field="hTotal" value="1"/>
    <usesource source="CompDivine"/>
    <tag group="fCategory" tag="General"/>
    <tag group="fShowWhat" tag="Freeform"/>
    <tag group="Helper" tag="ShowCharge"/>
    <tag group="Helper" tag="ShowSpec"/>
    </thing>
  <thing id="fWolvRage" name="Wolverine&apos;s Rage" description="You can fly into a berserk rage when injured.\n\n{b}Prerequisite{/b}: Wild shape\n\n{b}Benefit{/b}: If you have taken damage during the last round, you may spend a wild shape as a free action on your turn to enter a rage. While in this rage, you gain a +2 bonus to Strength, a +2 bonus to Constitution, and take a &#150;2 penalty to AC. This rage lasts for 5 rounds and cannot be ended voluntarily.\n\n{b}Source{/b}: Complete Divine" compset="Feat" summary="Free, if dam in prev rd, burn wild shape to rage (+2 Str &amp; Con, -2 AC) for 5 rds." uniqueness="useronce">
    <fieldval field="hName1" value="Raging"/>
    <usesource source="CompDivine"/>
    <tag group="Helper" tag="ShowSpec"/>
    <tag group="Helper" tag="ShowCharge"/>
    <tag group="fCategory" tag="Wild"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[      if (field[hIsOn1].value <> 0) then
        hero.child[aSTR].field[Bonus].value += 2
        hero.child[aCON].field[Bonus].value += 2
        hero.child[ArmorClass].field[Penalty].value -= 2
        endif]]></eval>
    </thing>
  <thing id="mmConSpell" name="Consecrate Spell" description="A spell you modify with this feat gains the good descriptor. Furthermore, if the spell deals damage, half of the damage (rounded down) results directly from divine power and is therefore not subject to be reduced by resistance or immunity to energy-based attacks. For example, a consecrated {i}fire storm{/i} spell cast by a 16th-level cleric deals 16d6 points of damage, half of which is fire damage and half of which is sheer divine power. Thus, creatures immune to fire still take damage. The consecrated spell uses up a spell slot one level higher than the spell&#146;s actual level." compset="Metamagic" summary="Half of spell&apos;s damage is divine power, ignoring energy resist/immunity. +1 Level." uniqueness="unique">
    <fieldval field="mmLevel" value="1"/>
    <fieldval field="mmAbbr" value="Consecrate"/>
    <usesource source="CompDivine"/>
    <usesource source="BoED"/>
    <prereq message="Consecrate Spell feat required.">
      <validate>@valid = hero.pickexists[fConsSpell] + parent.tagis[Helper.OnScroll]</validate>
      </prereq>
    </thing>
  <thing id="mmCorrSpel" name="Corrupt Spell" description="This feat adds the evil descriptor to a spell. Furthermore, if the spell deals damage, half of the damage (rounded down) results directly from divine power and is therefore not subject to be reduced by resistance or immunity to energy-based attacks. The corrupted spell uses up a spell slot one level higher than the spell&#146;s actual level." compset="Metamagic" summary="Half of spell&apos;s damage is divine power, ignoring energy resist/immunity. +1 Level." uniqueness="unique">
    <fieldval field="mmLevel" value="1"/>
    <fieldval field="mmAbbr" value="Corrupted"/>
    <usesource source="CompDivine"/>
    <prereq message="Corrupt Spell feat required.">
      <validate>@valid = hero.pickexists[fCorrSpell] + parent.tagis[Helper.OnScroll]</validate>
      </prereq>
    </thing>
  <thing id="mmRapidSpl" name="Rapid Spell" description="Only spells with a casting time greater than 1 standard action can be made rapid. A rapid spell with a casting time of 1 full round can be cast as a standard action. A rapid spell with a casting time measured in rounds can be cast in 1 full round. Rapid spells with casting times measured in minutes can be cast in 1 minute, and rapid spells with casting times measured in hours can be cast in 1 hour. A rapid spell uses up a spell slot one level higher than the spell&#146;s actual level." compset="Metamagic" summary="Full rd spells cast as standard, multi rd/min/hr cast in 1 rd/min/hr. +1 Level." uniqueness="unique">
    <fieldval field="mmLevel" value="1"/>
    <fieldval field="mmAbbr" value="Rapid"/>
    <usesource source="CompDivine"/>
    <prereq message="Rapid Spell feat required.">
      <validate>@valid = hero.pickexists[fRapidSpel] + parent.tagis[Helper.OnScroll]</validate>
      </prereq>
    </thing>
  <thing id="mmReachSpl" name="Reach Spell" description="You may cast a spell that normally has a range of touch at any distance up to 30 feet. The spell effectively becomes a ray, so you must succeed on a ranged touch attack to bestow the spell upon the recipient. A reach spell uses up a spell slot two levels higher than the spell&#146;s actual level." compset="Metamagic" summary="Touch range spells become a ranged touch with 30 ft range. +2 Levels." uniqueness="unique">
    <fieldval field="mmAbbr" value="Reach"/>
    <fieldval field="mmLevel" value="2"/>
    <usesource source="CompDivine"/>
    <tag group="User" tag="EasyMM"/>
    <prereq message="Reach Spell feat required.">
      <validate>@valid = hero.pickexists[fReachSpel] + parent.tagis[Helper.OnScroll]</validate>
      </prereq>
    <prereq message="Touch spell required.">
      <validate>@valid = tagis[sRange.Touch]</validate>
      </prereq>
    </thing>
  <thing id="xGrizzClaw" name="Grizzley&apos;s Claws" description="You can grow claws as sharp as those of a bear.\n\n{b}Prerequisite{/b}: Ability to wild shape.\n\n{b}Benefit{/b}: You can spend one wild shape to gain two primary claw attacks (both at your base attack bonus and adding your Strength bonus). The claws deal piercing and slashing damage equal to a short sword appropriate to your size (1d6 for Medium, or 1d4 for Small). The claws remain for 1 hour.\n\n{b}Special{/b}: If you already have a claw attack, this replaces those claws." compset="Special" summary="Burn wild shape to gain two primary claw attacks (XdX each) for 1 hour.">
    <comment>Still waiting on the 3d6 damage tag... (Done)</comment>
    <fieldval field="hName1" value="Active"/>
    <tag group="SpecSource" tag="Feat"/>
    <tag group="Helper" tag="ShowCharge"/>
    <tag group="Helper" tag="ShowSpec"/>
    <bootstrap thing="wClaw">
      <containerreq phase="First" priority="500"><![CDATA[count:User.GrizzClaw >=1]]></containerreq>
      <autotag group="Helper" tag="NatPrimary"/>
      <autotag group="Helper" tag="RaceSize"/>
      <autotag group="Value" tag="2"/>
      <autotag group="User" tag="GrizzClaw"/>
      </bootstrap>
    <eval phase="UserPostLv" priority="5000" index="2"><![CDATA[  if (herofield[tSize].value >= 4) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (4d6 each) for 1 hour."

  elseif (herofield[tSize].value = 3) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (3d6 each) for 1 hour."

  elseif (herofield[tSize].value = 2) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (2d6 each) for 1 hour."

  elseif (herofield[tSize].value = 1) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (1d8 each) for 1 hour."

  elseif (herofield[tSize].value = 0) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (1d6 each) for 1 hour."

  elseif (herofield[tSize].value = -1) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (1d4 each) for 1 hour."

  elseif (herofield[tSize].value = -2) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (1d3 each) for 1 hour."

  elseif (herofield[tSize].value = -3) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (1d2 each) for 1 hour."

  elseif (herofield[tSize].value <= -4) then
    field[xSumm].text = "Burn wild shape to gain two primary claw attacks (1 dam each) for 1 hour."
  endif]]></eval>
    <eval phase="First"><![CDATA[      if (field[hIsOn1].value <> 0) then
        perform hero.assign[User.GrizzClaw]
      endif]]></eval>
    <eval phase="PostLevel" priority="5000" index="3"><![CDATA[if (field[hIsOn1].value <> 0) then
   foreach pick in hero from BaseNatWep where "thingid.wClaw & User.GrizzClaw"
  if (herofield[tSize].value >= 4) then
    perform each.assign[wMain.4d6_106]

  elseif (herofield[tSize].value = 3) then
    perform each.assign[wMain.3d6_8]

  elseif (herofield[tSize].value = 2) then
    perform each.assign[wMain.2d6_104]

  elseif (herofield[tSize].value = 1) then
    perform each.assign[wMain.1d8_6]

  elseif (herofield[tSize].value = 0) then
    perform each.assign[wMain.1d6_5]

  elseif (herofield[tSize].value = -1) then
    perform each.assign[wMain.1d4_4]

  elseif (herofield[tSize].value = -2) then
    perform each.assign[wMain.1d3_3]

  elseif (herofield[tSize].value = -3) then
    perform each.assign[wMain.1d2_2]

  elseif (herofield[tSize].value <= -4) then
    perform each.assign[wMain.1_1]
  endif
 nexteach
endif]]></eval>
    </thing>
  <thing id="xSerpVenom" name="Serpent&apos;s Venom" description="You can deliver a toxic bite attack reminiscent of the viper.\n\n{b}Prerequisite{/b}: Ability to wild shape.\n\n{b}Benefit{/b}: You can spend a usage of wild shape to gain a secondary bite attack (at your base attack bonus &#150;5 and adding 1/2 your Strength bonus) that deals bludgeoning, piercing and slashing damage equal to a dagger appropriate to your size (1d4 for Medium, or 1d3 for Small). In addition, the bite delivers a toxic venom (Fortitude save DC 10 + &#189; your HD + your Con modifier; 1d6 Con/1d6 Con).\n\n{b}Special{/b}: If you already have a bite attack, it just gains the toxic venom part of this feat." compset="Special" summary="Burn wild shape, gain secondary bite att (XdX) with venom (DCYY, d6 Con/d6 Con).">
    <comment>Still waiting on the 3d6 damage tag...(Done)</comment>
    <fieldval field="hName1" value="Active"/>
    <tag group="SpecSource" tag="Feat"/>
    <tag group="Helper" tag="ShowCharge"/>
    <tag group="Helper" tag="ShowSpec"/>
    <bootstrap thing="wBite">
      <containerreq phase="First" priority="500"><![CDATA[count:User.SerpVenom >=1]]></containerreq>
      <autotag group="Helper" tag="NatSecond"/>
      <autotag group="Helper" tag="RaceSize"/>
      <autotag group="User" tag="SerpVenom"/>
      </bootstrap>
    <eval phase="UserPostAt" priority="5000" index="2"><![CDATA[   var DC as number
   DC = 10 + hero.child[aCON].field[aModBonus].value + (herofield[tHitDice].value / 2)
   DC = round(DC, 0, -1)

  if (herofield[tSize].value >= 4) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(3d6) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value = 3) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(2d6) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value = 2) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(1d8) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value = 1) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(1d6) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value = 0) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(1d4) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value = -1) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(1d3) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value = -2) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(1d2) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value = -3) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(1 dam) & venom(DC" & DC & ", d6 Con/d6 Con)."

  elseif (herofield[tSize].value <= -4) then
    field[xSumm].text = "Burn wild shape, gain secondary bite att(0 dam) & venom(DC" & DC & ", d6 Con/d6 Con)."
  endif]]></eval>
    <eval phase="First"><![CDATA[      if (field[hIsOn1].value <> 0) then
        perform hero.assign[User.SerpVenom]
      endif]]></eval>
    <eval phase="PostLevel" priority="5000" index="3"><![CDATA[if (field[hIsOn1].value <> 0) then
 foreach pick in hero from BaseNatWep where "thingid.wBite & User.SerpVenom"
  if (herofield[tSize].value >= 4) then
    perform each.assign[wMain.3d6_8]

  elseif (herofield[tSize].value = 3) then
    perform each.assign[wMain.2d6_104]

  elseif (herofield[tSize].value = 2) then
    perform each.assign[wMain.1d8_6]

  elseif (herofield[tSize].value = 1) then
    perform each.assign[wMain.1d6_5]

  elseif (herofield[tSize].value = 0) then
    perform each.assign[wMain.1d4_4]

  elseif (herofield[tSize].value = -1) then
    perform each.assign[wMain.1d3_3]

  elseif (herofield[tSize].value = -2) then
    perform each.assign[wMain.1d2_2]

  elseif (herofield[tSize].value = -3) then
    perform each.assign[wMain.1_1]

  elseif (herofield[tSize].value <= -4) then
    done
  endif
 nexteach
endif]]></eval>
    </thing>
  <thing id="xSwimFish" name="Swim Like a Fish" description="You can breathe and swim underwater with grace.\n\n{b}Prerequisite{/b}: Ability to wild shape.\n\n{b}Benefit{/b}: You can spend one wild shape to grow gills, enabling you to breathe underwater (while retaining your ability to breathe air). Webbing also grows between your fingers and between your toes, granting you a Swim speed of 40 ft. and a +8 bonus on your Swim checks. The effect lasts for one hour." compset="Special" summary="Burn wild shape, gain 40 ft Swim speed and ability to breathe water for 1 hr.">
