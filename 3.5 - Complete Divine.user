  <thing id="fSpontHeal" name="Spontaneous Healer" description="{b}Prerequisites{/b}: Knowledge (religion) 4 ranks, non-evil alignment, able to cast any {i}cure wounds{/i} spell.\n\n{b}Benefit{/b}: You can use your spellcasting ability to spontaneously cast {i}cure{/i} spells (from your class spell list) just as a cleric can. You may use this ability a total number of times per day equal to your Wisdom modifier.\n\n{b}Source{/b}: Complete Divine" compset="Feat" summary="X/day, you can spontaneously cast {i}cure{/i} spells." uniqueness="useronce">
    <usesource source="CompDivine"/>
    <tag group="fCategory" tag="General"/>
    <tag group="Helper" tag="ShowCharge"/>
    <tag group="Helper" tag="ShowSpec"/>
    <eval phase="UserPostAt" priority="10000"><![CDATA[   field[hTotal].value += hero.child[aWIS].field[aModBonus].value
   field[xSumm].text = "" & field[hTotal].value & "/day, you can spontaneously cast {i}cure{/i} spells."]]></eval>
    <exprreq message="Knowledge (religion) 4 ranks required."><![CDATA[#skillranks[kKnowRel] >= 4]]></exprreq>
    <exprreq message="Non-evil alignment required.">hero.tagis[Alignment.Evil] = 0</exprreq>
    <prereq message="Ability to cast any {i}cure wounds{/i} spell required.">
      <validate><![CDATA[ @valid = 0
~ Archivists, Clerics and Druids can cast Healing spells from 1st level.
    validif (tagcount[Classes.Archivist] + tagcount[Classes.Cleric] + tagcount[Classes.Druid] <> 0)

~ Paladins must be 4th level with an attribute bonus, or else 6th level to cast thier first Healing spell (cure light wounds).
 if (hero.childfound[cHelpPal].field[cMagicLev].value >= 6) then
    @valid = 1
 elseif (hero.childfound[cHelpPal].field[cMagicLev].value >= 4) then
   if (hero.childfound[cHelpPal].field[cSplAttBon].value >= 1) then
    @valid = 1
   endif
 endif

~ Rangers must be 8th level with an attribute bonus, or else 10th level to cast thier first Healing spell (cure light wounds).
 if (hero.childfound[cHelpRgr].field[cMagicLev].value >= 10) then
    @valid = 1
 elseif (hero.childfound[cHelpRgr].field[cMagicLev].value >= 8) then
   if (hero.childfound[cHelpRgr].field[cSplAttBon].value >= 2) then
    @valid = 1
   endif
 endif

~ Bards must be 2nd level with an attribute bonus, or else 3rd level to cast thier first Healing spell (cure light wounds).
 if (hero.childfound[cHelpBrd].field[cMagicLev].value >= 3) then
    @valid = 1
 elseif (hero.childfound[cHelpBrd].field[cMagicLev].value >= 2) then
   if (hero.childfound[cHelpBrd].field[cSplAttBon].value >= 1) then
    @valid = 1
   endif
 endif]]></validate>
      </prereq>
    </thing>
  <thing id="fSpontWoun" name="Spontaneous Wounder" description="{b}Prerequisites{/b}: Wis 13, Knowledge (religion) 4 ranks, non-good alignment, able to cast any {i}inflict wounds{/i} spell.\n\n{b}Benefit{/b}: You can use your spellcasting ability to spontaneously cast {i}inflict{/i} spells (from your class spell list) just as a cleric can. You may use this ability a total number of times per day equal to your Wisdom modifier.\n\n{b}Source{/b}: Complete Divine" compset="Feat" summary="X/day, you can spontaneously cast {i}inflict{/i} spells." uniqueness="useronce">
