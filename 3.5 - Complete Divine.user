  <thing id="cDOUncDod" name="Uncanny Dodge" description="You retain your Dexterity bonus to AC even if caught flat-footed or hit by an invisible attacker, but still lose it if immobilized." compset="ClSpecial" summary="Retain Dex bonus to AC when flat-footed.">
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <tag group="ClSpecWhen" tag="4"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="SpecSource" tag="cHelpDOr"/>
    <tag group="SpecType" tag="Defense" name="Defense" abbrev="Defense"/>
    <bootstrap thing="xUncanny"></bootstrap>
