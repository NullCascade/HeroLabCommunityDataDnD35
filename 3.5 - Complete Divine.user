    <tag group="SpecSource" tag="cHelpFaS"/>
    <tag group="Helper" tag="SpecUp"/>
    <eval phase="PostLevel" priority="10000"><![CDATA[doneif (tagis[Helper.SpcReplace] <> 0)
      
      ~ If we haven't chosen anything, get out now
      doneif (hero.child[cFaSDeiFaW].field[usrChosen1].ischosen = 0)

