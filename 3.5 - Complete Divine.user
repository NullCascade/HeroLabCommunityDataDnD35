  <thing id="cTmRTrapS1" name="Trap Sense +1" description="Bonus on reflex saves and AC against traps." compset="ClSpecial">
    <comment>Test</comment>
    <tag group="ClSpecWhen" tag="1"/>
    <tag group="Helper" tag="SpecUp" name="SpecUp" abbrev="SpecUp"/>
    <tag group="SpecSource" tag="cHelpTmR"/>
    <tag group="AbilType" tag="Extra" name="Extraordinary Ability" abbrev=" (Ex)"/>
    <bootstrap thing="xTrapSense"></bootstrap>
    <eval phase="PostLevel" priority="10000"><![CDATA[      ~ If we're not shown, just get out now
      if (tagis[Helper.ShowSpec] = 0) then
        done
        endif

      ~ Adjust our total bonus
      var bonus as number
      if (field[xTotalLev].value >= 10) then
        bonus = 4
      elseif (field[xTotalLev].value >= 7) then
        bonus = 3
      elseif (field[xTotalLev].value >= 4) then
        bonus = 2
      else
        bonus = 1
      endif

      hero.child[xTrapSense].field[Value].value += bonus]]></eval>
    </thing>
