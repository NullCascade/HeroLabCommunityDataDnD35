 var bonus as number
 var name as number

 if (level >= 17) then
    bonus = 3
    name = 3
 elseif (level >= 12) then
    bonus = 2
    name = 2
 elseif (level >= 7) then
    bonus = 1
    name = 1
 else
    bonus = 0
    name = 1
 endif

      field[livename].text = "Natural armor +" & name & ""
      hero.child[mNatural].field[Bonus].value = hero.child[mNatural].field[Bonus].value + bonus]]></eval>
    </thing>
  <thing id="cDrSEneImm" name="Energy Immunity" description="At 9th level, you gain immunity to the energy type of the breath weapon you gained at 4th level." compset="ClSpecial" summary="You gain immunity to the energy type of your breath weapon.">
