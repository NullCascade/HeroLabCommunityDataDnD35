    <tag group="gType" tag="Tools" name="Tools and Skill Kits" abbrev="Tools"/>
    </thing>
  <thing id="fJesterKit" name="Jester&apos;s Kit" description="A fine collection of gaudy clothing, face paint, toys, puppets, juggling balls, and other colorful items. It grants a +1 circumstance bonus on any Perform (comedy), Perform (buffoonery), or Disguise checks to avoid recognition." compset="Gear" stacking="merge">
