    <tag group="Helper" tag="GearSize" name="GearSize" abbrev="GearSize"/>
    <tag group="gType" tag="Tools" name="Tools and Skill Kits" abbrev="Tools"/>
    </thing>
  <thing id="fNaviKit" name="Navigator&apos;s Kit" description="This expensive set of instruments includes a sextant, astrolabe, compass, and measuring tools. It has no benefit on land, but grants a +2 circumstance bonus on Profession (Navigator) checks while at sea, and Profession (cartographer) checks to make sea charts." compset="Gear" stacking="merge">
    <fieldval field="gWeight" value="8"/>
    <fieldval field="gCost" value="250"/>
    <usesource source="AaEG"/>
    <tag group="gType" tag="Tools" name="Tools and Skill Kits" abbrev="Tools"/>
    <tag group="Helper" tag="GearSize"/>
    </thing>
  <thing id="fScryKit" name="Scryer&apos;s Kit" description="This is a variety of focusing crystals, incense, mirrors, and other tools to help a scryer concentrate. It grants a +2 circumstance bonus on Scry checks. The user must have the ability to cast the {i}scrying{/i} spell or have a crystal ball.\n\n(Arms and Equipment Guide 3.0 Version, now defunct since 3.5 eliminated the Scry skill. Included here for completeness&apos; sake.)" compset="Gear" stacking="merge">
    <fieldval field="gWeight" value="2"/>
    <fieldval field="gCost" value="40"/>
    <usesource source="AaEG"/>
    <tag group="Helper" tag="GearSize" name="GearSize" abbrev="GearSize"/>
    <tag group="gType" tag="Tools" name="Tools and Skill Kits" abbrev="Tools"/>
    </thing>
  <thing id="fSwimKit" name="Swimmer&apos;s Kit" description="This consists of a snorkel (see separate entry, bonus already factored in here), crude goggles, flippers, and occasionally a skintight suit. It grants a +2 circumstance bonus on Swim checks, and cannot be worn with other clothing or armor." compset="Gear" stacking="merge">
    <fieldval field="gWeight" value="5"/>
    <fieldval field="gCost" value="15"/>
    <usesource source="AaEG"/>
    <tag group="Helper" tag="GearSize" name="GearSize" abbrev="GearSize"/>
    <tag group="gType" tag="Tools" name="Tools and Skill Kits" abbrev="Tools"/>
    </thing>
  <thing id="fOutAnmTrn" name="Outfit, Animal Training" description="Essentially beefed up padded armor, this suit covers a person from head to toe in thick, quilted padding, especially around the limbs. It is used as protection by those who specialize in the training of attack animals, such as dogs. It is very difficult to move in this outfit, so it does not make for good armor.\n\nUsed as armor, it has the following statistics: armor bonus +2, maximum Dex bonus +1, armor check penalty -7, arcane spell failure 40%, slows as medium armor." compset="Gear" stacking="merge">
    <fieldval field="gCost" value="10"/>
