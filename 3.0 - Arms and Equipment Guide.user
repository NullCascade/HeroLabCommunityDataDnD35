    <tag group="EquipType" tag="Wood"/>
    <tag group="mArmorChk" tag="0"/>
    <tag group="mArcFail" tag="0"/>
    </thing>
  <thing id="mBone" name="Bone" description="Bone armor is a cloth or leather coat reinforced with strips of bone, often cut from animals used for food. The armor covers the torso, but leaves the limbs free for better mobility.\n\nThis armor can be donned in 1 minute, or 5 rounds if donned hastily. It may be removed in 1 minute (or half that time with help)\n\n{b}Source{/b}: Arms and Equipment Guide" compset="Armor">
    <fieldval field="gSizeCost" value="20"/>
    <fieldval field="gWeight" value="20"/>
    <usesource source="AaEG"/>
