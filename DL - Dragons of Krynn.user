  <thing id="rAurDomPer" name="Dominate Person" description="An aurak can cast the {i}dominate person{/i} spell once per a day at caster level 8th (Will DC15+Cha modifier)" compset="Special" summary="Cast Dominate Person once per day" uniqueness="unique">
