    <eval phase="First" priority="10500"><![CDATA[var breed as number
breed = hero.childfound[fRvnHFBree].field[fChosen].chosen.field[Value].value

if (breed = 1) then
 field[ftCandExpr].text = "thingid.xRvnHFDRE | thingid.xRvnHFInWi"
elseif (breed = 2) then
 field[ftCandExpr].text = "thingid.xRvnHFDRC | thingid.xRvnHFEndu"
elseif (breed = 5) then
 field[ftCandExpr].text = "thingid.xRvnHFDRA | thingid.xRvnHFDRC | thingid.xRvnHFDRE | thingid.xRvnHFDRF | thingid.xRvnHFDRS | thingid.xRvnHFInWi"
elseif (breed = 6) then
 field[ftCandExpr].text = "thingid.xRvnHFDRF | thingid.xRvnHFMoWi"
elseif (breed = 7) then
 field[ftCandExpr].text = "thingid.xRvnHFDRE | thingid.xRvnHFInWi"
endif]]></eval>
    <eval phase="First" priority="10750" index="2">doneif (field[fChosen].ischosen = 0)

~set hTotal field for bootstrapping purposes
field[hTotal].value = field[fChosen].chosen.field[Value].value</eval>
    </thing>
